{
  "[Markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[groovy]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.detectIndentation": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[lua]": {
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[svelte]": {
    "editor.detectIndentation": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "comments": false,
      "other": true,
      "strings": true
    },
    "editor.tabSize": 2
  },
  "colorInfo.languages": [
    {
      "colors": "css",
      "selector": "css"
    },
    {
      "colors": "css",
      "selector": "sass"
    },
    {
      "colors": "css",
      "selector": "scss"
    },
    {
      "colors": "css",
      "selector": "less"
    },
    {
      "colors": "css",
      "selector": "jsonc"
    }
  ],
  "prettier.requireConfig": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "editor.cursorSurroundingLines": 10,
  "editor.fontFamily": "FiraCode Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnSaveMode": "modifications",
  "editor.formatOnType": false,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": true,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": true,
  "editor.lineHeight": 16,
  "editor.minimap.enabled": true,
  "editor.multiCursorModifier": "alt",
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    88
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
  "emmet.excludeLanguages": [
    "markdown",
    "typescriptreact",
    "typescript"
  ],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "explorer.confirmDelete": false,
  "files.associations": {
    "*.txt": "shellscript",
    "*Jenkinsfile*": "groovy",
    "*bashrc": "shellscript",
    "*jenkinsfile*": "groovy",
    "*zshrc": "shellscript"
  },
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 10000,
  "files.defaultLanguage": "json",
  "files.exclude": {
    "**/*.map": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.svn": true,
    "**/.vscode": true,
    "**/CVS": true,
    "**/__pycache__": true,
    "**/env/*": true,
    "**/envkins/*": true,
    "**/envune/*": true,
    "**/fuck/*": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.metals": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.path": "git",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.keymap": "chorded",
  "indentRainbow.colors": [
    "#aea8d318",
    "#e26a6a18",
    "#ffd07a18",
    "#fc639918",
    "#ffecdb18",
    "#6495ed18",
    "#00e0e018",
    "#f4d03f18",
    "#b8860b18"
  ],
  "peacock.affectAccentBorders": true,
  "peacock.affectTabActiveBorder": true,
  "peacock.favoriteColors": [
    {
      "name": "Lavender Marriage",
      "value": "#aea8d3"
    },
    {
      "name": "Fuck You Papaya",
      "value": "#e26a6a"
    },
    {
      "name": "Bougie Bitch Yellow",
      "value": "#ffd07a"
    },
    {
      "name": "Pepto Bismol Shots",
      "value": "#fc6399"
    },
    {
      "name": "Creamed",
      "value": "#ffecdb"
    },
    {
      "name": "Powder Keg Blue",
      "value": "#6495ed"
    },
    {
      "name": "Drowned in Seafoam green",
      "value": "#00e0e0"
    },
    {
      "name": "SCREAMING YELLA",
      "value": "#f4d03f"
    },
    {
      "name": "Grubby Gold",
      "value": "#1857a4"
    }
  ],
  "peacock.showColorInStatusBar": true,
  "peacock.surpriseMeFromFavoritesOnly": true,
  "peacock.surpriseMeOnStartup": true,
  "python.analysis.diagnosticMode": "openFilesOnly",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.ignorePatterns": [
    ".vscode/*.py",
    "**/site-packages/**/*.py"
  ],
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/node_modules": true,
    "dist": true,
    "jspm_packages": true
  },
  "terminal.external.osxExec": "kitty",
  "vim.insertModeKeyBindings": [
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "j",
        "k"
      ]
    },
    {
      "after": [
        "<Esc>"
      ],
      "before": [
        "k",
        "j"
      ]
    }
  ],
  // "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  // "vscode-neovim.neovimInitVimPaths.linux": "/home/kardia/.config/nvim/init.vim",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#ff00aa3d",
    "editor.selectionHighlightBackground": "#ff00aa3d",
    "editorRuler.foreground": "#2ee2fa33"
  },
  "workbench.colorTheme": "Gruvbox Dark Hard",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.useSplitJSON": true,
  "workbench.sideBar.location": "right"
}
