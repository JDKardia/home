# BEGIN SpaceCommander config for manage.stripe.net
Host sshbastion-*.global.*.manage.stripe.net
    Port 30001
    UserKnownHostsFile ~/.ssh/machine-cas
    StrictHostKeyChecking yes
    PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com

Host *.manage.stripe.net !*.qa.manage.stripe.net !qa-*.manage.stripe.net !sshbastion-*.global.prod.manage.stripe.net
    ProxyJump sshbastion-prod-peered.global.prod.manage.stripe.net

# For hosts in the QA management domain
Host *.qa.manage.stripe.net qa-*.manage.stripe.net !sshbastion-*.global.qa.manage.stripe.net
    ProxyJump sshbastion-qa-peered.global.qa.manage.stripe.net

Host *.manage.stripe.net !sshbastion-*.global.*.manage.stripe.net
    Port 30000
    UserKnownHostsFile ~/.ssh/machine-cas
    SendEnv LANG LC_* SC_USER
    StrictHostKeyChecking yes
    ServerAliveInterval 15

Match host *.manage.stripe.net user root
    PubkeyAcceptedKeyTypes *
Match host *.manage.stripe.net !user root
    PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
# END SpaceCommander config for manage.stripe.net

# BEGIN SpaceCommander config for stripe.io
Host *.stripe.io !qa-*.stripe.io !*.qa.stripe.io !preprod-*.stripe.io !*.preprod.stripe.io
    ProxyJump sshbastion-prod-peered.global.prod.manage.stripe.net
Host preprod-*.stripe.io *.preprod.stripe.io
    ProxyJump sshbastion-preprod-peered.global.prod.manage.stripe.net
Host qa-*.stripe.io *.qa.stripe.io
    ProxyJump sshbastion-qa-peered.global.prod.manage.stripe.net

Match host *.stripe.io,*.nw,*.east,*.consul
    PubkeyAcceptedKeyTypes ssh-rsa-cert-v01@openssh.com,ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
    Port 30000
    UserKnownHostsFile ~/.ssh/machine-cas
    StrictHostKeyChecking yes
    SendEnv LANG LC_* SC_USER
    ServerAliveInterval 15
host git.corp.stripe.com
user git
hostname git.corp.stripe.com
port 22222
userknownhostsfile ~/.ssh/known_hosts-stripe.io
ServerAliveInterval 15
ServerAliveCountMax 60
PubkeyAcceptedKeyTypes -*-cert-v01@openssh.com

host git.qa.corp.stripe.com
user git
hostname git.qa.corp.stripe.com
port 22222
userknownhostsfile ~/.ssh/known_hosts-stripe.io
ServerAliveInterval 15
ServerAliveCountMax 60
PubkeyAcceptedKeyTypes -*-cert-v01@openssh.com

host not-a-git.corp.stripe.com
user git
hostname not-a-git.corp.stripe.com
port 33333
userknownhostsfile ~/.ssh/known_hosts-stripe.io
ServerAliveInterval 15
ServerAliveCountMax 60
PubkeyAcceptedKeyTypes -*-cert-v01@openssh.com

host not-a-git.qa.corp.stripe.com
user git
hostname not-a-git.qa.corp.stripe.com
port 33333
userknownhostsfile ~/.ssh/known_hosts-stripe.io
ServerAliveInterval 15
ServerAliveCountMax 60
PubkeyAcceptedKeyTypes -*-cert-v01@openssh.com

# END SpaceCommander config for stripe.io

# BEGIN Global SSH bastion config (go/ssh-bastions)
Host sshbastion*.global.*.manage.stripe.net
  Port 30001
  StrictHostKeyChecking yes
  UserKnownHostsFile ~/.ssh/machine-cas
  PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
Host sshbastion*.global.prod.manage.stripe.net
  HostKeyAlias sshbastion.global.prod.manage.stripe.net
Host sshbastion*.global.qa.manage.stripe.net
  HostKeyAlias sshbastion.global.qa.manage.stripe.net
# END Global SSH bastion config

Host *
  AddKeysToAgent yes
Host github
  hostname github.com
  IdentityFile ~/.ssh/id_ed25519
