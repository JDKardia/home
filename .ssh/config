# BEGIN SpaceCommander config for uptycs.stripe.net
Host *.prod.uptycs.stripe.net *.uptycs.stripe.net !qa-*.uptycs.stripe.net !*.qa.uptycs.stripe.net
    ProxyJump sshbastion-prod-peered.global.prod.manage.stripe.net
Host *.qa.uptycs.stripe.net qa-*.uptycs.stripe.net
    ProxyJump sshbastion-qa-peered.global.prod.manage.stripe.net

Match host *.uptycs.stripe.net
    Port 30000
    UserKnownHostsFile ~/.ssh/machine-cas
    StrictHostKeyChecking yes
    SendEnv LANG LC_* SC_USER
    ServerAliveInterval 15

Match host *.uptycs.stripe.net user root
    PubkeyAcceptedKeyTypes *
Match host *.uptycs.stripe.net !user root
    PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
# END SpaceCommander config for uptycs.stripe.net

# BEGIN SpaceCommander config for internalsystems.stripe.net
Host *.prod.internalsystems.stripe.net *.internalsystems.stripe.net !qa-*.internalsystems.stripe.net !*.qa.internalsystems.stripe.net
  ProxyJump sshbastion-prod-peered.global.prod.manage.stripe.net
Host *.qa.internalsystems.stripe.net qa-*.internalsystems.stripe.net
  ProxyJump sshbastion-qa-peered.global.prod.manage.stripe.net

Host sshbastion*.global.prod.manage.stripe.net
  HostKeyAlias sshbastion.global.prod.manage.stripe.net
  Port 30001
  UserKnownHostsFile ~/.ssh/machine-cas
  StrictHostKeyChecking yes
  PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com

Host *.internalsystems.stripe.net
  Port 30000
  UserKnownHostsFile ~/.ssh/machine-cas
  StrictHostKeyChecking yes
  SendEnv LANG LC_* SC_USER
  ServerAliveInterval 15

Match host *.internalsystems.stripe.net user root
  PubkeyAcceptedKeyTypes *
Match host *.internalsystems.stripe.net !user root
  PubkeyAcceptedKeyTypes ecdsa-sha2-nistp256-cert-v01@openssh.com,ssh-ed25519-cert-v01@openssh.com
# END SpaceCommander config for internalsystems.stripe.net

